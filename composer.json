{
    "name": "blinkpay/laravel",
    "description": "BlinkPay payment gateway integration for Laravel",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "BlinkPay\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlinkPay\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlinkPay\\Laravel\\BlinkPayServiceProvider"
            ]
        }
    }
} 