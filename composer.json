{
    "name": "blinkpay/laravel",
    "description": "BlinkPay payment gateway integration for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BlinkPay",
            "email": "info@blink.co.ug"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0.0|^9.0.0|^10.0.0",
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BlinkPay\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlinkPay\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlinkPay\\Laravel\\BlinkPayServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "laravel/framework": "<8.0"
    }
} 